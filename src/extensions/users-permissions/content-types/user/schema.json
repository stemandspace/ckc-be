{
  "kind": "collectionType",
  "collectionName": "up_users",
  "info": {
    "name": "user",
    "description": "",
    "singularName": "user",
    "pluralName": "users",
    "displayName": "User"
  },
  "options": {
    "draftAndPublish": false
  },
  "attributes": {
    "username": {
      "type": "string",
      "minLength": 3,
      "unique": true,
      "configurable": false,
      "required": true
    },
    "email": {
      "type": "email",
      "minLength": 6,
      "configurable": false,
      "required": true
    },
    "provider": {
      "type": "string",
      "configurable": false
    },
    "password": {
      "type": "password",
      "minLength": 6,
      "configurable": false,
      "private": true,
      "searchable": false
    },
    "resetPasswordToken": {
      "type": "string",
      "configurable": false,
      "private": true,
      "searchable": false
    },
    "confirmationToken": {
      "type": "string",
      "configurable": false,
      "private": true,
      "searchable": false
    },
    "confirmed": {
      "type": "boolean",
      "default": false,
      "configurable": false
    },
    "blocked": {
      "type": "boolean",
      "default": false,
      "configurable": false
    },
    "role": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.role",
      "inversedBy": "users",
      "configurable": false
    },
    "firstname": {
      "type": "string"
    },
    "lastname": {
      "type": "string"
    },
    "grade": {
      "type": "string"
    },
    "parentname": {
      "type": "string"
    },
    "dob": {
      "type": "string"
    },
    "bio": {
      "type": "text"
    },
    "notification_daily": {
      "type": "boolean",
      "default": true
    },
    "general_notification": {
      "type": "boolean",
      "default": true
    },
    "payment_notification": {
      "type": "boolean",
      "default": true
    },
    "referral_id": {
      "type": "uid"
    },
    "avatar": {
      "type": "string"
    },
    "banner": {
      "type": "text"
    },
    "school_name": {
      "type": "string"
    },
    "address": {
      "type": "text"
    },
    "private": {
      "type": "boolean",
      "default": false
    },
    "premium": {
      "type": "biginteger"
    },
    "credits": {
      "type": "biginteger",
      "default": "15",
      "min": "0"
    },
    "mobile": {
      "type": "biginteger"
    },
    "level": {
      "type": "biginteger"
    },
    "purchases": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::purchase.purchase",
      "mappedBy": "user"
    },
    "coins": {
      "type": "biginteger",
      "default": "0",
      "min": "0"
    },
    "achivements": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::achivement.achivement",
      "mappedBy": "user"
    },
    "setup": {
      "type": "boolean",
      "default": false
    },
    "type": {
      "type": "enumeration",
      "enum": [
        "free",
        "basic",
        "premium"
      ],
      "default": "free"
    },
    "country": {
      "type": "string"
    },
    "state": {
      "type": "string"
    },
    "city": {
      "type": "string"
    }
  }
}
