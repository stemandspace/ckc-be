    "singularName": "daily-quiz",
    "pluralName": "daily-quizs",
    "displayName": "Daily Quiz",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true
    },
