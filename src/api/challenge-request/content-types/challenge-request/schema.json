{
  "kind": "collectionType",
  "collectionName": "challenge_requests",
  "info": {
    "singularName": "challenge-request",
    "pluralName": "challenge-requests",
    "displayName": "Challenge Request",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "mediaUrl": {
      "type": "text"
    },
    "status": {
      "type": "enumeration",
      "enum": [
        "pending",
        "approved",
        "rejected"
      ]
    },
    "challenge_id": {
      "type": "biginteger"
    },
    "user": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "plugin::users-permissions.user"
    },
    "winner": {
      "type": "boolean",
      "default": false
    }
  }
}
