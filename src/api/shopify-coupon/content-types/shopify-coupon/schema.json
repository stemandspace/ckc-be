{
  "kind": "collectionType",
  "collectionName": "shopify_coupons",
  "info": {
    "singularName": "shopify-coupon",
    "pluralName": "shopify-coupons",
    "displayName": "Shopify Coupon",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "code": {
      "type": "string"
    },
    "user": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "plugin::users-permissions.user"
    },
    "start": {
      "type": "date"
    },
    "end": {
      "type": "date"
    },
    "oneTime": {
      "type": "boolean"
    },
    "configuration": {
      "type": "json"
    },
    "title": {
      "type": "string"
    },
    "expires": {
      "type": "boolean",
      "default": false
    },
    "discount": {
      "type": "string"
    },
    "discountType": {
      "type": "enumeration",
      "enum": [
        "fixed_amount",
        "percentage"
      ]
    },
    "shopify_price_rule": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::shopify-price-rule.shopify-price-rule"
    }
