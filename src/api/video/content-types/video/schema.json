{
  "kind": "collectionType",
  "collectionName": "videos",
  "info": {
    "singularName": "video",
    "pluralName": "videos",
    "displayName": "Video",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true
    },
    "desc": {
      "type": "richtext",
      "required": true
    },
    "price": {
      "type": "decimal",
      "required": true
    },
    "premium": {
      "type": "boolean",
      "default": false,
      "required": true
    },
    "duration": {
      "type": "biginteger"
    },
    "grade": {
      "type": "string"
    },
    "mentor": {
      "type": "string"
    },
    "slug": {
      "type": "uid",
      "targetField": "title",
      "required": true
    },
    "mediaUrl": {
      "type": "string",
      "required": true
    },
    "tags": {
      "type": "string"
    },
    "rewards": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::reward.reward",
      "mappedBy": "video"
    },
    "thumbnail": {
      "type": "media",
      "multiple": true,
      "required": true,
      "allowedTypes": [
        "images"
      ]
    },
    "quiz": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::quiz.quiz"
    },
    "new": {
      "type": "boolean",
      "default": false
    },
    "trending": {
      "type": "boolean",
      "default": false
    }
  }
}
